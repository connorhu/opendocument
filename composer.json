{
    "name": "connorhu/open-document",
    "description": "PHP Library to read / write ODF 1.3",
    "require": {
        "php": ">=8.3",
        "ext-xmlwriter": "*",
        "ext-zip": "*"
    },
    "require-dev": {
        "ext-dom": "*",
        "symfony/var-dumper": "^6.4",
        "symfony/filesystem": "^6.4"
    },
    "autoload": {
        "psr-4": {
            "OpenDocument\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OpenDocument\\Tests\\": "tests/"
        }
    }
}
